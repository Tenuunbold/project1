num1 = float(input("enter the first number: "))
num2 = float(input("enter the second number: "))
result = num1 + num2
result2 = num1 / num2
result3 = num1 * num2
result4 = num1 - num2
print(f"Enter sum of {num1} and {num2} is {result}")
print(f"Enter divide of {num1} and {num2} is {result2}")
print(f"Enter sum of {num1} and {num2} is {result4}")
print(f"Enter mul of {num1} and {num2} is {result3}")
